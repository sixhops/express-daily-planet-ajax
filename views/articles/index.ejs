<link rel="stylesheet" type="text/css" href="/css/styles.css">
<h1>Articles</h1>

<a href="/articles/new" class="btn btn-primary">New Article</a>

<% articles.forEach(function(article, id) { %>
  <div class="well">
	<a href="/articles/<%= id + 1 %>/edit">edit </a><a href="/articles/<%= id + 1 %>" class="delete">delete </a><a href="/articles/<%= id + 1 %>"><h2><%= article.title %></h2></a>
  </div>
<% }); %>

<script src="//code.jquery.com/jquery-3.3.1.min.js"></script>
<script src="/js/script.js"></script>