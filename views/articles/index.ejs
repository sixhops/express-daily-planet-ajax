<h1>Articles</h1>
<br/>
<a href="/articles/new" class="btn btn-primary">Add New Article</a>

<% articles.forEach(function(article, id) { %>
  <div class="well">
    <a href="/articles/<%= article.id %>"><br/><h2><%= article.title %></h2></a>
    <a class="btn btn-primary" href="/articles/<%= article.id %>/edit">Edit this Article</a>
    <a href="/articles/<%= article.id %>" class="btn btn-danger delete">Delete</a>
    
  </div>
<% }); %>
